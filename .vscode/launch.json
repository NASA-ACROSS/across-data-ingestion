{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "local: Start Data Ingestion",
            "type": "debugpy",
            "request": "launch",
            "envFile": "${workspaceFolder}/.env",
            "python": "${workspaceFolder}/.venv/bin/python",
            "pythonArgs": [
                "-Xfrozen_modules=off"
            ],
            "module": "uvicorn",
            "args": [
                "across_data_ingestion.main:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "8001"
            ],
        },
        {
            "name": "dev: Start Data Ingestion",
            "type": "debugpy",
            "request": "launch",
            "envFile": "${workspaceFolder}/.env",
            "python": "${workspaceFolder}/.venv/bin/python",
            "pythonArgs": [
                "-Xfrozen_modules=off"
            ],
            "env": {
                "APP_ENV": "across-plat-ue2-dev",
                "RUNTIME_ENV": "dev",
                "AWS_PROFILE": "Project-Admin",
            },
            "module": "uvicorn",
            "args": [
                "across_data_ingestion.main:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "8001"
            ],
        },
    ]
}
